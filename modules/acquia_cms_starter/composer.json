{
    "name": "drupal/acquia_cms_starter",
    "type": "drupal-module",
    "description": "Contains the example content for demonstration.",
    "license": "GPL-2.0-or-later",
    "require": {
        "drupal/acquia_cms_article": "dev-1.x",
        "drupal/acquia_cms_document": "dev-1.x",
        "drupal/acquia_cms_event": "dev-1.x",
        "drupal/acquia_cms_page": "dev-1.x",
        "drupal/acquia_cms_search": "dev-1.x",
        "drupal/acquia_cms_video": "dev-1.x"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true,
            "phpro/grumphp-shim": true,
            "webdriver-binary/binary-chromedriver": true
        }
    }
}
